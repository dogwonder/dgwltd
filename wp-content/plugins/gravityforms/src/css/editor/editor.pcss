/* Variables */
@import "../variables/index.pcss";

/* Components */
@import "../components/_inputs.pcss";
@import "../components/_errors.pcss";
@import "../components/_labels.pcss";
@import "../components/_conditional_flyout.pcss";

/* Shared */
@import "../shared/_grid.pcss";
@import "../shared/_complex-fields.pcss";

/* Base */
@import "../shared/_fonts.pcss";

/** Font **/

td, button, a, div, li {
	font-family: var(--t-font-family-base);
	line-height: var(--t-line-height-base);
	-webkit-font-smoothing: antialiased;
}
/** Page layout **/

.toplevel_page_gf_edit_forms {
	background-color: var( --c-white );
}

body.toplevel_page_gf_edit_forms #wpfooter{
	display: none;
}

body.toplevel_page_gf_edit_forms #wpcontent{
	padding-left: 0px;
}

body.toplevel_page_gf_edit_forms #wpbody-content{
	width: 100%;
}

body.toplevel_page_gf_edit_forms #gform-form-toolbar{
	margin: 0 0 0 0;
	box-shadow: none;
}

body.toplevel_page_gf_edit_forms .gforms_edit_form  {
	bottom: 0;
	display: flex;
	flex-grow: 1;
	height: 100vh;
	margin: 0 0 0 0 !important;
	overscroll-behavior-y: none;
	position: fixed;
	top: 32px;
	width: 100%;
}

body.toplevel_page_gf_edit_forms .gforms_edit_form > table {
	padding-top: 3.75rem;
	background-color: var( --c-white );
}

#form_editor_fields_container{
	background-color: transparent;
	box-sizing: border-box;
	display: flex;
	flex-direction: column;
	flex-grow: 1;
	max-height: calc( 100% - 88px );
	max-width: calc( 100% - 530px );
	min-height: calc( 100% - 88px );
	overflow-x: hidden;
	overflow-y: auto;
	padding: 2rem 2rem 2rem 2rem;
	position: relative;
	right: 0;
	top: 3.75rem;
	z-index: 1;
}

#form_editor_fields_container.droppable {
	max-width: 100%;
	padding: 2rem 562px 2rem 2rem;
	z-index: 2;
}

.folded #form_editor_fields_container {
	left: 36px;
}

/* In form editor, hide anything that is not related to form editing ( notices and update alerts .. etc ), unless explicitly defined */
#wpbody-content > div:not( .gforms_edit_form ):not(.gforms_force_editor_display) {
	display: none !important;
}

/** Toolbar **/

#gform-form-toolbar .form_switcher_arrow {
	margin-left: 0.5rem;
}

#gf_toolbar_menu_container {
	display: flex;
	height: 36px;

	a {
		display: inline-flex;
		border-radius: 2px;
		padding: 6px;
		margin: 0 !important;
		font-family: var(--t-font-family-base);
		font-size: var(--t-font-size-base);
		color: var( --c-port-dark );
		background: none;
		cursor: pointer;
		border: none;
		outline: none;
		transition: color 0.2s ease, box-shadow .1s linear;

		svg {
			stroke: currentColor;
		}

		&:active {
			color: inherit;
		}

		&:focus {
			box-shadow: 0 0 0 1.5px var( --c-gravity-blue );
			outline: 1px solid transparent;
		}

		&:hover:not(:active){
			color: var( --c-gravity-blue );
		}
	}
}

.gform-form-switcher__container{
	top: 87px !important;
}

body.toplevel_page_gf_edit_forms #gform-form-toolbar {
	align-items: center;
	background: var( --c-white );
	border-bottom: 1px solid var( --c-white-lilac );
	box-shadow: none;
	box-sizing: border-box;
	color: var( --c-port-dark );
	display: flex;
	font-size: var(--t-font-size-base);
	justify-content: start;
	margin: 0 0 0 0;
	max-height: 3.75rem;
	min-height: 3.75rem;
	padding: 0 0.5rem 0 0;
	position: fixed;
	width: calc(100% - 160px);
	z-index: 99;
	-webkit-font-smoothing: antialiased;
}

body.toplevel_page_gf_edit_forms.folded #gform-form-toolbar{
	width: calc(100% - 36px);
}

.gform-form-toolbar {
	.gform-form-toolbar__logo {
		/* hidden for now, display only when in fullscreen mode */
		display: none;

		a {
			align-items: center;
			background: var( --c-port );
			display: flex;
			height: 3.813rem;
			justify-content: center;
			transition: all ease-in-out 190ms;
			width: 3.813rem;

			&:hover {
				background: var( --c-chathams );

				svg path.g {
					fill: var( --c-chathams );
				}
			}
		}
	}

	.gform-form-toolbar__form-title {
		color: var( --c-text );
		font-family: inherit;
		font-size: var(--t-font-size-base);
		font-style: normal;
		font-weight: var(--t-font-weight-normal);
		height: 1.25rem;
		letter-spacing: unset;
		line-height: var(--t-line-height-base);
		margin-right: 0;
		padding: 0 1.65rem 0 1.25rem;
	}

	#gform-form-toolbar__menu {
		display: flex;
		height: 3.75rem;
	}

	& > li {
		margin-bottom: 0;
		padding-top: 0.5625rem;
	}

	.gform-form-toolbar__divider {
		display: inline-block;
		height: 100%;
		width: 1px;
		background: var( --c-white-lilac );
		margin: 0 0.8125rem;
	}

	a:focus {
		box-shadow: none;
		outline: none;
	}
}

.gform-form-toolbar__form-title a:focus {
	box-shadow: none;
	outline: none;
}

#gform-form-toolbar__menu li > a:hover {
	border-bottom-color: var( --c-gravity-blue );
}

.gform-form-toolbar__form-title a.form_switcher_arrow svg {
	fill: currentColor;
}

.gform-form-toolbar__form-title a.form_switcher_arrow {
	color: var( --c-port-dark );
}

.gform-form-toolbar__form-title a.form_switcher_arrow:hover {
	color: var( --c-button );
}

#gform-form-toolbar__menu {
	a.gf_toolbar_active {
		cursor: default;
	}

	& > li > a {
		color: var( --c-port-dark );
		font: inherit;
	}

	& > li > a.gf-topmenu-dynamic svg {
		fill: currentColor;
	}

	& > li > a.gf-topmenu-dynamic {
		line-height: 1px;
		padding-top: 13px;
		border-bottom: none;
	}

	.gform-form-toolbar__icon {
		width: 1.25rem;
		text-align: center;
		margin-right: .5rem;
		display: inline-flex;
		align-content: center;

		svg {
			max-width: 1.25rem;
		}
	}
}

/** Popover **/

.gf-popover{
	display: none;
	position: absolute;
	top: 45px;
	animation: components-animate__appear-animation 0.1s cubic-bezier(0, 0, 0.2, 1) 0s;
	animation-fill-mode: forwards;
}

@keyframes components-animate__appear-animation {
	from {
		transform: translateY(-2em) scaleY(0) scaleX(0); }
	to {
		transform: translateY(0%) scaleY(1) scaleX(1); }
}

.gf-popover::after{
	border-bottom-color:rgb(255, 255, 255);
	border-bottom-style:solid;
	border-bottom-width:8px;
	border-image-outset:0px;
	border-image-repeat:stretch;
	border-image-slice:100%;
	border-image-source:none;
	border-image-width:1;
	border-left-color:rgba(0, 0, 0, 0);
	border-left-style:solid;
	border-left-width:8px;
	border-right-color:rgba(0, 0, 0, 0);
	border-right-style:solid;
	border-right-width:8px;
	border-top-color:rgb(68, 68, 68);
	border-top-style:none;
	border-top-width:0px;
	content:"";
	display:block;
	height:0px;
	line-height:0px;
	margin-left:-10px;
	position:absolute;
	top: -6px;
	width: 0px;
	left: 18.5px;
}
.gf-popover::before{
	border-bottom-color:rgb(226, 228, 231);
	border-bottom-style:solid;
	border-bottom-width:8px;
	border-image-outset:0px;
	border-image-repeat:stretch;
	border-image-slice:100%;
	border-image-source:none;
	border-image-width:1;
	border-left-color:rgba(0, 0, 0, 0);
	border-left-style:solid;
	border-left-width:8px;
	border-right-color:rgba(0, 0, 0, 0);
	border-right-style:solid;
	border-right-width:8px;
	border-top-color:rgb(68, 68, 68);
	border-top-style:none;
	border-top-width:0px;
	content:"";
	display:block;
	height:0px;
	line-height:0px;
	margin-left:-10px;
	position:absolute;
	top:-8px;
	width:0px;
	left: 18.5px;
}
.gf-popover__content {
	padding-top: 0.5rem;
	padding-bottom: 0.5rem;
	box-shadow: 0 3px 30px rgba(25, 30, 35, 0.1);
	border: 1px solid #e2e4e7;
	background: #fff;
	height: auto;
	overflow-y: auto;
	width: 200px;
	border-bottom: 1px solid #e2e4e7;
}

.gf-popover__button:focus {
	color: #1E1E1E;
	border: none;
	box-shadow: none;
	outline-offset: -2px;
	outline: 1px dotted #1E1E1E;
}
.gf-popover__button:hover {
	color: #1E1E1E;
	border: none;
	box-shadow: none;
	background: #f3f4f5;
}
.gf-popover__button {
	display: flex;
	width: 100%;
	padding: 8px 15px;
	text-align: left;
	color: #40464d;
	border: none;
	box-shadow: none;
	text-decoration: none;
	font-size: var(--t-font-size-base);
	cursor: pointer;
	background: none;
	border-radius: 4px;
	transition: box-shadow 0.1s linear;
}

.gf-popover__button :first-child{
	margin-right: 4px;
}
/** Tabs **/

div.ui-tabs div.ui-tabs-panel{
	border: none;
}

div.ui-widget-content {
	background-color: var( --c-white );
}

/** Sidebar **/

.editor-sidebar {
	height: 100%;
	position: fixed;
	right: 0;
	top: 92px;
	z-index: 1;
}

.sidebar  {
	width:370px;
	border-left: 1px solid var( --c-white-lilac );
	border-bottom: 1px solid var( --c-white-lilac );
	max-height: calc(100% - 85px);
	min-height: calc(100% - 85px);
	background-color: var( --c-white );
	padding: 0;
}

.sidebar .sidebar__nav{
	background-color: inherit;
	border-bottom: 1px solid var( --c-white-lilac ) !important;
	height: 129px;
	padding: 0px ;
	position: fixed;
	width: 370px;
	z-index: 1000;
}

.sidebar .sidebar-instructions {
	padding: 21px 23px 21px 57px;
	position: relative;
}

.sidebar .sidebar-instructions p {
	margin: 0;
}

.sidebar .sidebar-instructions:before {
	content: url(data:image/svg+xml;base64,);
	height: 1.0625rem;
	left: 1.6875rem;
	line-height: 1.25rem;
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	width: 1.0625rem;
}

.sidebar .field_settings,
.sidebar .panel-block-tabs {
	overflow-y: auto;
}

.sidebar .sidebar__nav li.sidebar__nav__item.ui-state-default.ui-state-active,
.sidebar .sidebar__nav li.sidebar__nav__item.ui-state-default {
	background-color: transparent ;
	border: none ;
	margin: 0 ;
	border-radius: 0px ;
	min-width: 107px;
}

.sidebar .sidebar__nav li.sidebar__nav__item.ui-state-default a, #gform-form-toolbar_links li a {
	box-shadow: none ;
	color: var( --c-text );
	display: block;
	font-size: var(--t-font-size-base);
	font-weight: var(--t-font-weight-normal);
	line-height: 45px ;
	outline: none;
	padding: 0px ;
	text-align: center;
	vertical-align: middle;
	outline: none;
	box-shadow: none ;
	width: 100%;
}

.sidebar .sidebar__nav li.sidebar__nav__item {
	a::after {
		transition: all .15s linear;
	}

	&.ui-state-default {
		a::after {
			background: transparent;
			border-radius: 3px 3px 0 0;
			content: " ";
			display: block;
			height: 2px;
			transition: all .15s linear;
			width: 100%;
		}
	}

	&.ui-state-hover {

		a::after {
			background: var(--c-snuff);
			border-radius: 3px 3px 0 0;
			content: " ";
			display: block;
			height: 3px;
			width: 100%;
			transition: all .15s linear;
		}

	}

	&.ui-state-default.ui-state-active a::after {
		background: var(--c-gravity-blue);
		border-radius: 3px 3px 0 0;
		content: " ";
		display: block;
		height: 3px;
		width: 100%;
	}

}

.sidebar .sidebar__nav li.sidebar__nav__item.ui-state-default.ui-state-active a:focus,
.sidebar .sidebar__nav li.sidebar__nav__item.ui-state-default.ui-state-active a:active {
	box-shadow: none ;
}

.panel-block-tabs__toggle {

	.ui-accordion-header-active {
		margin-bottom: 5px;
	}

	i:before{
		color:	var(--c-amethyst-smoke);
		content: "\f347";
	}

	&:hover {
		background: var(--c-light-blue);

		i:before{
			color:	var(--c-port);
		}

	}

	&:focus {
		border-color: var(--c-primary-light);
	}

}

.panel-block-tabs__toggle.ui-accordion-header-active i:before{
	content: "\f343";
}

.panel-block-tabs__toggle i {
	font-family: 'dashicons';
	color: var( --c-amethyst-smoke );
	font-style: normal;
	position: absolute;
	right: 1rem;
	font-size: 18px;
}

.search-button {
	padding: 1rem;
	position: relative;
}
.search-button span {
	content: url("data:image/svg+xml;base64,");
	height: 1.0625rem;
	line-height: 1.25rem;
	position: absolute;
	right: 2rem;
	top: 50%;
	transform: translateY(-50%);
	width: 1.0625rem;
}

.search-button:hover span {
	content: url(data:image/svg+xml;base64,);
}

.search-button.clearable .clear-button {
	content: url(data:image/svg+xml;base64,);
	max-width: 0.75rem;
	max-height: 0.75rem;
	padding-right: 0.3175rem;
}

.search-button.clearable:hover .clear-button {
	content: url(data:image/svg+xml;base64,);
}

html[dir="rtl"] {
	.search-button span {
		left: 2rem;
		right: initial;
	}
}

.add-buttons,  .panel-block-tabs__body{
	border: none;
	border-left: none ;
	padding: 14px;
	width: auto ;
}

html[dir="rtl"] .search-button:after {
	left: 1.75rem;
	right: auto;
}

html[dir="rtl"] .add-buttons {
	justify-content: right;
}

.gf-field-group__no-results {
	padding: 0 1rem;
	font-size: 0.75rem;
	font-style: italic;
}

.panel-block-tabs__body--nopadding{
	padding: 1rem 0 0 0;
}

.panel-block-tabs__body--settings {

	label {
		margin-bottom: 0.5rem;
	}

	.field-choice-row label {
		margin-bottom: 0rem;
	}
}

.add-buttons {

	display: flex;
	flex-wrap: wrap;
	align-content: stretch;
	justify-content: left;
	overflow: visible;
	margin: 0;

	li {
		margin-bottom: 13px ;
		width: 33.33%;

		button {
			align-items: stretch;
			background: transparent;
			border-radius: 3px;
			border: 1px solid var(--c-snuff);
			color: var(--c-text);
			cursor: pointer;
			display: flex;
			flex-direction: column;
			font-size: var(--t-font-size-base);
			height: 5.8125rem;
			justify-content: space-around;
			margin: 0 auto;
			padding: 0.5rem 0 0 0;
			width: 6.1875rem;
			word-break: break-word;
			transition: 0.1s all ease-in-out;

			&:hover {
				border-color: var(--c-gravity-blue);
				color: var(--c-gravity-blue);
			}

			&:focus {
				border:	1px solid var(--c-primary-light);
				border-radius: 3px;
				box-shadow: 0px 0px 0px 2px var(--c-focus);
				color: var(--c-port);
				outline: none;
				transition: box-shadow .15s ease;
			}

		}

		button.fieldPlaceholder {
			border: 1px dashed var(--c-snuff);

			div {
				opacity: 0;
			}

		}

	}
}

button.ui-draggable-dragging {
	align-items: stretch;
	background: var(--c-white);
	border: 1px solid var(--c-snuff);
	border-radius: 3px;
	box-shadow: 0px 0px 1px rgba(18, 25, 97, 0.24), 0px 24px 24px rgba(18, 25, 97, 0.03), 0px 2px 2px rgba(18, 25, 97, 0.03), 0px 4px 4px rgba(18, 25, 97, 0.03), 0px 8px 8px rgba(18, 25, 97, 0.03), 0px 16px 16px rgba(18, 25, 97, 0.03);
	color: var(--c-port);
	cursor: grabbing;
	display: flex;
	flex-direction: column;
	font-size: var(--t-font-size-base);
	justify-content: space-around;
	padding: 0.5rem 0 0 0;
	text-align: center;
	white-space: normal;
	z-index: 999999999;
}

html[dir="rtl"] div.gforms_edit_form button.ui-draggable-dragging,
html[dir="rtl"] div.gforms_edit_form button.ui-draggable-dragging div {
	text-align: center !important;
}

button .button-text {
	font-weight: var(--t-font-weight-normal);
	font-size: 0.75rem;
	line-height: 1rem;
	padding: 0 0 0.5rem;
	text-align: center;
}

.button-icon {
	border-radius: 3px;
	font-family: 'dashicons';
	font-size: 21px;
	height: 24px;
	text-align: center;
	width: 100%;

	img {
		width: 1.3125rem;
		color: var( --c-text );
		fill: var( --c-text );
	}

	&:before {
		line-height: 1.5rem;
		transition: all 0.1s ease-in-out;
	}

}

.button-icon.icon-default:before{
	content: "\f111";
}

.sidebar .sidebar__panel{
	padding: 131px 0 0 ;
}

.sidebar__panel.ui-tabs-panel {
	text-align: left;
	display: flex;
	flex-flow: column;
	height: calc( 100vh - 220px );
}

.panel-block--flex {
	display: flex;
}

.panel-block--hidden {
}

.panel-block--flex {
	display: flex;
}

.panel-block--hidden {
	display: none ;
}

#nothing_selected {
	padding: 25px;
	text-align: center;
}

li.pagination_setting,
li.field_setting {
	margin-bottom:0.9375rem;
}

.input_active_icon {
	margin:0.3rem 0 0 0 ;
}

.field-choice-row {
