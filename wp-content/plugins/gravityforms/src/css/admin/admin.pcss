/* ---------------------------------------------------
	Form Toolbar
--------------------------------------------------- */

#gform-form-toolbar {

	background: #FFF;
	box-shadow: 0 1px 4px rgba( 58, 58, 87, 0.0829654 ), 0 1px 0 rgba( 44, 44, 64, 0.0615659 );
	transition: top .3s ease;
	width: 100%;

}

.gform-form-toolbar--fixed {

	position: fixed;
	z-index: 3;

}

.gform-form-toolbar__container {

	display: flex;
	flex-wrap: wrap;
	margin-left: auto;
	margin-right: auto;
	max-width: 69.5rem;
	padding: 0 1.5rem;

}

.gform-form-toolbar__form-title,
.gform-form-toolbar__form-id {

	color: #242748;
	font-weight: 600;
	line-height: 3.75rem;
	padding-bottom: 3px;

}

.gform-form-toolbar__form-title {

	font-size: .875rem;
	letter-spacing: 0.45px;
	margin-right: 1.625rem;

}

.gform-form-toolbar__form-title span {

	display: inline-block;
	line-height: 1.5;
	overflow: hidden;
	text-overflow: ellipsis;
	vertical-align: middle;
	white-space: nowrap;
	max-width: 10.375rem;

}

.gform-form-toolbar__form-id {

	font-size: .8125rem;
	margin-left: auto;

}

#gform-form-toolbar__menu {
	& > li {
		display: inline-block;
		margin: 0;
	}

	& > li > a {
		align-items: center;
		color: var( --c-text );
		cursor: pointer;
		display: inline-flex;
		height: 3.75rem;
		padding: 0 0.8125rem 0 0.8125rem;
		text-decoration: none;
		transition: all .2s ease;
	}

	a.gf_toolbar_active {
		font-weight: 700;
	}

	.gform-form-toolbar__submenu {

		visibility: hidden;
		opacity: 0;
		margin-top: -0.3125rem;
		min-width: 14.688rem;
		position: absolute;
		transition: 200ms all;
		z-index: 100;

		&.open {
			visibility: visible;
			opacity: 1;
		}

		ul {
			background: #FFF;
			border: 1px solid var( --c-blue-haze );
			border-radius: 3px;
			box-shadow: 0px 10px 15px rgba(58, 61, 90, 0.1), 0px 4px 6px rgba(58, 61, 90, 0.05);
			box-sizing: border-box;
			max-height: calc( 100vh - 10rem );
			overflow-y: scroll;
		}

		li {
			display: block;
			margin: 0;
		}

	}

}

.row-actions {
	.gform-form-toolbar__icon {
		display: none;
	}

	.gform-form-toolbar__submenu {
		display: none;
		position: absolute;
		background: #FFF;
		border: 1px solid #C3C5DB;
		border-radius: 3px;
		box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.05), 0px 10px 15px rgba(0, 0, 0, 0.1);
		box-sizing: border-box;
		max-height: calc(100vh - 35rem);
		overflow: auto;
	}
}

.gform-form-toolbar__submenu a,
.row-actions .gform-form-toolbar__submenu a {

	color: var( --c-text );
	cursor: pointer;
	display: flex;
	align-items: center;
	padding: 0.9075rem 0.9375rem;
	text-decoration: none;
	transition: all .2s ease;

}

.gform-form-toolbar__submenu a:hover,
.row-actions .gform-form-toolbar__submenu a:hover {

	background: var( --c-light-blue );

}

